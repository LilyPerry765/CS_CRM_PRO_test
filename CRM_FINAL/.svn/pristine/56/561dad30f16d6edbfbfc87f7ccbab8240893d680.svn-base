<Window x:Class="CRMConvertApplication.IbsngForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="IbsngForm" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Name="ServiceButton" Click="ServiceButton_Click" Width="100" Height="30" Content="شارژ"/>
        <Button Grid.Row="1" Name="TrafficButton" Click="TrafficButton_Click" Width="100" Height="30" Content="ترافیک"/>
        <Button Grid.Row="2" Name="CreateButton" Click="CreateButton_Click" Width="100" Height="30" Content="افزودن"/>
    </Grid>
</Window>

    