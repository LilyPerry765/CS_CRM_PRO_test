<Window x:Class="CRMConvertApplication.ADSLUserForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ADSLUserForm" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Name="StartButton" Click="StartButton_Click" Width="100" Height="50" Content="Start"/>

        <Button Grid.Row="1" Name="ReStartButton" Click="ReStartButton_Click" Width="100" Height="50" Content="ReStart"/>
        
        <Label Grid.Row="2" Name="CountLabel" Content="Count : "/>

        <Label Grid.Row="3" Name="CountSaveLabel" Content="Count Save : "/>
    </Grid>
</Window>
